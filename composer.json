{
    "name": "hashemi/queryfilter",
    "description": "A simple & dynamic package for your eloquent query in laravel. It will help you to write query logic individual for each parameter.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "filter",
        "query",
        "query-filters",
        "laravel"
    ],
    "authors": [
        {
            "name": "Hashemi Rafsan",
            "email": "rafsanhashemi@gmail.com"
        },
        {
            "name": "Syed Sirajul Islam Anik",
            "email": "sirajul.islam.anik@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/console": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hashemi\\QueryFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Helpers"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hashemi\\QueryFilter\\QueryFilterServiceProvider"
            ]
        }
    }
}
